#%%
#b)
consultaSQL = """
SELECT id_tipoevento AS descripcion
       anio AS año
       COUNT(cantidad) AS cantidad
FROM casos
GROUP BY descripcion, año 
ORDER BY descripcion ASC, año DESC
"""
res = sql^ consultaSQL 
print(res)

#%%
#c)
consultaSQL = """
SELECT id_tipoevento AS descripcion
       anio AS año
       COUNT(cantidad) AS cantidad
FROM casos
WHERE año = 2019
GROUP BY descripcion, año 
ORDER BY descripcion ASC, año DESC

"""
res= sql^ consultaSQL
print(res)

